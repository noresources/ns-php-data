{
	"name": "noresources/ns-php-data", 
	"description": "Data (de)serialization library", 
	"type": "library", 
	"license": "MIT", 
	"keywords": [
		"serialization"
	], 
	"authors": [
		{
			"name": "Renaud Guillard", 
			"email": "dev@nore.fr", 
			"role": "lead"
		}
	], 
	"autoload": {
		"psr-4": {
			"NoreSources\\Data\\": "src/"
		}
	}, 
	"require": {
		"php": ">=5.6", 
		"noresources/ns-php-mediatype": "^1.5"
	}, 
	"suggest": {
		"ext-json": "*", 
		"ext-yaml": "*"
	}, 
	"scripts": {
		"test": "@php  ./vendor/bin/phpunit --color=never --bootstrap vendor/autoload.php tests"
	},
	"autoload-dev": {
		"psr-4": {
			"NoreSources\\Data\\Test\\": "tests/src/"
		}
	}, 
	"require-dev": {
		"phpunit/phpunit": ">= 5.0"
	}, 
	"extra": {
		"branch-alias": {
			"dev-1.2.x": "1.2.x-dev",
			"dev-master": "1.3.x-dev"
		}
	}, 
	"bin": [
		"bin/data-convert.php"
	]
}
